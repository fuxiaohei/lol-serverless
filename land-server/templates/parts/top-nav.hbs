<div id="top-nav" class="border-bottom d-flex justify-content-between align-items-center p-3">
    <ol class="breadcrumb m-0 p-0 border-0 shadow-none d-flex align-items-center">
        {{#each page.breadcrumb}}
        {{#if link}}
        <li class="breadcrumb-item"><a class="text-reset" href="{{link}}">{{title}}</a></li>
        {{else}}
        <li class="breadcrumb-item active">{{title}}</li>
        {{/if}}
        {{/each}}
    </ol>
    <div class="right">right</div>
</div>